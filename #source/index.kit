      <main class="main">
        <section class="main__hero hero">
          <div class="hero__container">
            <div class="hero__content">
              <div class="hero__column">
                <div class="hero__main">
                  <h1 class="hero__title title title_h1">find the place to live <span>your dreams</span> easily here</h1>
                  <p class="hero__text">Everything you need about finding your place to live will be here, where it will be easier for you</p>
                  <form action="#" class="hero__search search-form _icon-location">
                    <input type="text" placeholder="Search for the location you want!" class="search-form__input" />
                    <button class="search-form__submit _icon-icon-right btn btn_green"> Search </button>
                  </form>
                  <div class="hero__partners partners">
                    <h3 class="partners__title title title_h3">Our Partnership</h3>
                    <div class="partners__logos">
                      <img class="partners__logo" src="images/hero/partners/traveloka_logo.png" alt="Logo Traveloca" />
                      <img class="partners__logo" src="images/hero/partners/ticket.com_logo.png" alt="Logo Ticket.com" />
                      <img class="partners__logo" src="images/hero/partners/airbnb_logo.png" alt="Logo Airbnb" />
                      <img class="partners__logo" src="images/hero/partners/tripadvisor_logo.png" alt="Logo Tripadvisor" />
                    </div>
                  </div>
                </div>
              </div>
              <div class="hero__column">
                <!-- Slider main container -->
                <div class="hero__slider swiper">
                  <!-- Additional required wrapper -->
                  <div class="swiper-wrapper">
                    <!-- Slides -->
                    <div class="swiper-slide slide">
                      <div class="slide__column">
                        <img src="images/hero/main-image/slider/slide-1/faces.png" alt="Various faces of Hounter clients" />
                      </div>
                      <div class="slide__column">
                        <h3 class="slide__title">1K+ People</h3>
                        <p class="slide__text">Successfully Getting Home</p>
                      </div>
                    </div>
                    <div class="swiper-slide slide">
                      <div class="slide__column">
                        <img src="images/hero/main-image/slider/slide-2/house.png" alt="Example of Hounter house sold" />
                      </div>
                      <div class="slide__column">
                        <h3 class="slide__title">56 Houses</h3>
                        <p class="slide__text">Sold Monthly</p>
                      </div>
                    </div>
                    <div class="swiper-slide slide">
                      <div class="slide__column">
